[src/**.java]
chatset = utf-8
end_of_line = lf
indent_style = space
indent_size = 2
insert_final_newline = true
ij_java_continuation_indent_size = 2
ij_formatter_on_tag = @formatter::on
ij_formatter_off_tag = @formatter::off
ij_java_method_call_chain_wrap = split_into_lines
ij_any_array_initializer_new_line_after_left_brace = true


[**.{yml|yaml}]
chatset = utf-8
end_of_line = lf
indent_style = space
indent_size = 2
insert_final_newline = true
